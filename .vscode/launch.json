{
	"type": "node",
	"request": "launch",
	"name": "Run with ts-node",
	"runtimeArgs": ["-r", "ts-node/register"],
	"args": ["${workspaceFolder}/index.ts"],
	"skipFiles": ["<node_internals>/**"],
	"env": {
		"TS_NODE_PROJECT": "${workspaceFolder}/tsconfig.json"
	},
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Run ts-node",
			"runtimeExecutable": "ts-node",
			"cwd": "${workspaceFolder}",
			"args": []
		}
	]
}
